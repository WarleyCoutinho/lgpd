d\"\n                v-model=\"dataInicial\"\n                required\n                hide-header\n                dark\n                placeholder=\"Data Inicial\"\n                :date-format-options=\"{\n                  year: 'numeric',\n                  month: '2-digit',\n                  day: '2-digit',\n                }\"\n              ></b-form-datepicker>\n            </b-form-group>\n          </b-col>\n          <b-col md=\"6\">\n            <b-form-group label=\"Visualizar:\" label-for=\"executar-id\">\n              <b-button\n                class=\"card-bg-color form-control\"\n                id=\"executar-id\"\n                @click.prevent=\"loadFiltro()\"\n              >\n                <i class=\"fa fa-eye\"></i> Visualizar\n              </b-button>\n            </b-form-group>\n          </b-col>\n        </b-row>\n      </b-col>\n    </b-row>\n  </div>\n</template>\n\n<script>\nimport { showError, filtroOEE } from '@/global';\n\nimport PageTitle from '../../components/template/PageTitle';\n\nexport default {\n  name: 'Filtro',\n  components: {\n    PageTitle,\n  },\n  data() {\n    return {\n      filtro: {},\n      dataInicial: '',\n    };\n  },\n  methods: {\n    loadFiltro() {\n      if (this.dataInicial === '') {\n        showError('Selecione a Data Inicial');\n        return;\n      }\n\n      this.filtro.dataInicial = this.dataInicial;\n      localStorage.setItem(filtroOEE, JSON.stringify(this.filtro));\n      this.$emit('loadGraphic');\n    },\n  },\n};\n</script>\n\n<style></style>\n"]}]}